{
  "name": "端末制限ツールプロトタイプ",
  "manifest_version": 2,
  "version": "1.0",
  "browser_action": {
    "default_title": "端末制限ツールプロトタイプ",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "open.png",
    "48": "open48.png",
    "128": "open128.png"
  },
  "background": {
    "scripts": ["background.js","hmac-sha1.js"]
  },
   "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "https://localhost/deviceControl"
  ]
}
